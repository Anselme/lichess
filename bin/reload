#!/bin/sh

bin/build_bootstrap.php

for environment in dev test; do
    rm -rf lichess/cache/$environment/*
    php lichess/console --env=$environment --debug -v cache:warmup
    php lichess/console --env=$environment --debug -v doctrine:mongodb:data:load
    php lichess/console --env=$environment --debug -v doctrine:mongodb:schema:create --index
    php lichess/console --env=$environment --debug -v cache:warmup
done
