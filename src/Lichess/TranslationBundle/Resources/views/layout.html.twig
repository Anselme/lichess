{% extends "LichessBundle::layout.html.twig" %}
{% block stylesheets %}
{{ parent() }}
{% stylesheets
    '@LichessBundle/Resources/public/css/translation.css'
    filter="yui_css" %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
{% endstylesheets %}
{% endblock %}
{% block javascripts %}
{{ parent() }}
{% javascripts
    '@LichessBundle/Resources/public/js/translation.js'
    filter="yui_js" %}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
{% block layout_content %}
<div class="content_box">{% block content %}{% endblock %}</div>
{% endblock %}
