{% extends "LichessBundle::layout.html.twig" %}
{% block stylesheets %}
{{ parent() }}
{% stylesheets
    '@LichessUserBundle/Resources/public/css/user.css'
    '@LichessBundle/Resources/public/css/gamelist.css'
    filter="yui_css" %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
{% endstylesheets %}
{% endblock %}
{% block javascripts %}
{{ parent() }}
{% javascripts
    '@LichessUserBundle/Resources/public/js/user.js'
    filter="yui_js" %}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
{% block layout_content %}
<div id="lichess_user">{% block content %}{% endblock %}</div>
{% endblock %}
{% block menu_active %}user{% endblock %}
