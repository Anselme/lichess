<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="lichess.timeline.pusher.class">Bundle\LichessBundle\Timeline\Pusher</parameter>
    </parameters>

    <services>
        <service id="lichess.timeline.pusher" class="%lichess.timeline.pusher.class%">
            <argument type="service" id="lichess.repository.timeline_entry" />
            <argument type="service" id="templating" />
        </service>
    </services>

</container>
