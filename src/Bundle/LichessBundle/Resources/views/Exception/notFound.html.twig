{% extends "Lichess:Exception:base.html.twig" %}
{% block error_title %}{% trans 'Page not found' %} ({{ status_code }}: {{ status_text }}){% endblock %}
{% block error_text %}{% trans "There is nothing to see here. If you think it's a bug, you could send an email to %email%" with {'%email%': '<span class="js_email"></span>'} %}{% endblock %}
